<p id="notice"><%= notice %></p>
<table class="table table-striped">
  <thead>
    <tr>
      <th>TITLE/BUILDING NAME</th>
      <th>SUBMITTED BY</th>
      <th>DATE</th>
      <th>STATUS</th>
      <th>VERSION</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room.each do |room| %>
      <tr>
        <td><%= room.name %></td>
        <td><%= room.user.username %></td>
        <td><%= room.created_at.to_date %></td>
        <td><%= room.status %></td>
        <td><%=  %></td>
        <td><%= link_to 'APPROVE' , approve_path(room)%></td>
        <td><%= link_to 'REJECT' %></td>
        <td><%= link_to 'VIEW' %></td>
      </tr>
    <% end %>
  </tbody>
</table>